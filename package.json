{
  "name": "findaneighbor-admin",
  "version": "0.0.1",
  "main": "index.html",
  "scripts": {
    "start": "MODE=development parcel serve src/index.html",
    "build": "MODE=production parcel build src/index.html --no-scope-hoist",
    "stage": "now && now rm findaneighbor-admin -ys",
    "prod": "now --prod && now rm findaneighbor-admin -ys"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/tehpsalmist/findaneighbor-admin.git"
  },
  "keywords": [
    "parcel",
    "react",
    "tailwindcss",
    "template"
  ],
  "author": "Ben Steward <bstewardcodes@gmail.com> (https://tehpsalmist.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tehpsalmist/findaneighbor-admin/issues"
  },
  "homepage": "https://github.com/tehpsalmist/findaneighbor-admin#readme",
  "devDependencies": {
    "autoprefixer": "^10.2.5",
    "parcel": "^2.8.1",
    "tailwindcss": "^2.0.4"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.5",
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "apollo-boost": "^0.4.9",
    "apollo-link-ws": "^1.0.20",
    "auth0-spa-js": "^1.6.5",
    "graphql": "^14.7.0",
    "react": "latest",
    "react-dom": "latest",
    "react-router-dom": "^5.2.0",
    "react-select": "^3.1.1",
    "react-smooth-dnd": "^0.11.1",
    "subscriptions-transport-ws": "^0.9.18"
  },
  "app": "public/index.html",
  "targets": {
    "main": false,
    "app": {
      "engines": {
        "browsers": "Chrome 70"
      }
    }
  }
}
